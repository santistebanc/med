{
  "name": "med",
  "version": "0.1.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node index.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=prod && node index.js",
    "start_windows": "set NODE_ENV=prod&&node index.js",
    "linked-modules": "find node_modules/ -maxdepth 1 -type l -ls | grep -o -E \"[^/]+$\"",
    "save-linked-modules": "npm run -s linked-modules > .linked_modules",
    "link-modules": "cat .linked_modules | xargs yarn link",
    "docker:build": "docker build -t med .",
    "docker:clean": "docker rm -f med || true",
    "docker:run": "docker run -p 7880:3000 -d --link med-mongo:mongo --name med med && npm run docker:logs",
    "docker:stop": "docker stop med",
    "docker:start": "docker start med && npm run docker:logs",
    "docker:logs": "docker logs -f med",
    "deploy": "npm run docker:build && npm run docker:clean && npm run docker:run"
  },
