{
  "name": "staart-example",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node --harmony-async-await index.js",
    "build": "next build",
    "start": "export NODE_ENV=prod && node --harmony-async-await index.js",
    "linked-modules": "find node_modules/ -maxdepth 1 -type l -ls | grep -o -E \"[^/]+$\"",
    "save-linked-modules": "npm run -s linked-modules > .linked_modules",
    "link-modules": "cat .linked_modules | xargs yarn link",
    "docker:build": "docker build -t staart .",
    "docker:clean": "docker rm -f staart || true",
    "docker:run": "docker run -p 7880:3000 -d --link mongo:mongo --name staart staart && npm run docker:logs",
    "docker:stop": "docker stop staart",
    "docker:start": "docker start staart && npm run docker:logs",
    "docker:logs": "docker logs -f staart",
    "deploy": "npm run docker:build && npm run docker:clean && npm run docker:run"
  },
  "author": "Nicola Marcacci Rossi <nicolamr@gmail.com> (http://nmr.io)",
  "license": "ISC",
  "dependencies": {
    "compose-next": "^0.8.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "graphql": "^0.10.3",
    "graphql-server-express": "^1.0.0",
    "graphql-tools": "^1.1.0",
    "mailcomposer": "^4.0.2",
    "mailgun-js": "^0.11.2",
    "mongodb": "^2.2.30",
    "next": "^2.4.7",
    "ooth": "^0.8.4",
    "ooth-client": "^0.8.0",
    "ooth-client-react": "^0.8.0",
    "ooth-client-react-next": "^0.8.4",
    "ooth-local": "^0.8.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-input-range": "^1.2.1",
    "staart": "^0.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "glob": "^7.1.2",
    "node-sass": "^4.5.3",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5"
  }
}